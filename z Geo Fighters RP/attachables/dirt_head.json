{
    "format_version": "1.10.0",
    "minecraft:attachable": {
      "description": {
        "identifier": "hog:dirt_head.player",
        "item": { "hog:dirt_helmet": "query.is_owner_identifier_any('minecraft:player')" },
        "materials": {
          "default": "armor",
          "enchanted": "armor_enchanted"
        },
        "textures": {
          "default": "textures/blocks/dirt",
          "enchanted": "textures/misc/enchanted_item_glint"
        },
        "geometry": {
          "default": "geometry.block_head"
        },
        "scripts": {
          "parent_setup": "variable.helmet_layer_visible = 1.0;"
        },
        "render_controllers": [ "controller.render.armor" ]
      }
    }
  }
  